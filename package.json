{
   "name": "dna.js",
   "version": "1.3.5",
   "description": "An uncomplicated user interface library for semantic templates",
   "summary": "dna.js is an easy-to-use UI library for jQuery enabling developers to rapidly build maintainable JavaScript applications.",
   "license": "MIT",
   "main": "dna.js",
   "repository": "dnajs/dna.js",
   "homepage": "https://github.com/dnajs/dna.js",
   "bugs": "https://github.com/dnajs/dna.js/issues",
   "docs": "http://dnajs.org/docs",
   "keywords": [
      "clone",
      "dom",
      "engine",
      "html",
      "jquery",
      "json",
      "template",
      "templating",
      "ui"
   ],
   "author": {
      "name": "Center Key",
      "url": "http://centerkey.com"
   },
   "jshintConfig": {
      "esversion": 6,
      "strict": "implied",
      "undef": true,
      "unused": true,
      "node": true,
      "mocha": true
   },
   "scripts": {
      "pretest": "jshint gulpfile.js spec/*.js && gulp build",
      "test": "mocha spec/*.js",
      "web": "gulp web"
   },
   "dependencies": {
      "jquery": ">=2.2"
   },
   "devDependencies": {
      "del": "~3.0",
      "gulp": "~3.9",
      "gulp-file-include": "~2.0",
      "gulp-header": "~2.0",
      "gulp-htmlhint": "~2.1",
      "gulp-jshint": "~2.1",
      "gulp-rename": "~1.2",
      "gulp-replace": "~0.6",
      "gulp-size": "~3.0",
      "gulp-uglify": "~3.0",
      "gulp-w3cjs": "~1.3",
      "jsdom": "~11.6",
      "jshint": "~2.9",
      "mocha": "~5.0"
   }
}
